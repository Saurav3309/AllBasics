<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    tools:context=".sixth"
   >


    <TextView
        android:id="@+id/getName1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#121010"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/getEmail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text=""
        android:textColor="#232121"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/getName1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/getName1"
        app:layout_constraintTop_toBottomOf="@+id/getName1" />

    <TextView
        android:id="@+id/getAddress"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text=""
        android:textColor="#191616"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/getEmail"
        app:layout_constraintStart_toStartOf="@+id/getEmail"
        app:layout_constraintTop_toBottomOf="@+id/getEmail" />

<!--    <TextView-->
<!--        android:id="@+id/getCountry"-->
<!--        android:layout_width="0dp"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:layout_marginTop="20dp"-->
<!--        android:text=""-->
<!--        android:textColor="#131212"-->
<!--        android:textSize="16sp"-->
<!--        app:layout_constraintStart_toStartOf="@+id/getAddress"-->
<!--        app:layout_constraintTop_toBottomOf="@+id/getAddress"-->
<!--        app:layout_constraintWidth_percent="0.1" />-->

    <TextView
        android:id="@+id/getMobile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#191717"
        android:textSize="16sp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="@+id/getAddress"
        app:layout_constraintStart_toStartOf="@+id/getAddress"
        app:layout_constraintTop_toBottomOf="@+id/getAddress" />
    <Button
        android:id="@+id/sevenPage"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/click_here"
        android:textAllCaps="false"
        android:padding="16dp"
        app:layout_constraintStart_toStartOf="@+id/getMobile"
        app:layout_constraintTop_toBottomOf="@+id/getMobile"
        app:layout_constraintEnd_toEndOf="@+id/getMobile"
        app:layout_constraintWidth_percent="0.4"
        android:layout_marginTop="20dp"
        />





</androidx.constraintlayout.widget.ConstraintLayout>